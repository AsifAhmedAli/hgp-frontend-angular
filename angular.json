{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hgp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hgp/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/favicon.png",
              "src/assets"
            ],
            "styles": [
              "src/assets/plugins/font-awesome/css/font-awesome.min.css",
              "src/assets/plugins/bootstrap4/css/bootstrap.min.css",
              "src/assets/plugins/owl-carousel/assets/owl.carousel.css",
              "src/assets/plugins/jquery-bar-rating/dist/themes/fontawesome-stars.css",
              "src/assets/plugins/jquery-ui/jquery-ui.min.css",
              "src/assets/plugins/select2/dist/css/select2.min.css",
              "src/assets/fonts/Linearicons/Linearicons/Font/demo-files/demo.css",
              "src/assets/css/style.css",
              "src/assets/css/mega-menu.css",
              "src/assets/css/dev2.css",
              "src/assets/css/about-css.css",
              "src/assets/css/dev3.css",

              // "src/assets/plugins/slick/slick/slick.css",
              // "src/assets/plugins/slick/slick/slick-theme.css",
              "src/assets/css/webfonts/stylesheet.css",
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/ngx-lightbox/lightbox.css",
              "node_modules/select2/dist/css/select2.min.css",
              "node_modules/lightgallery.js/dist/css/lightgallery.css",
              "node_modules/lightgallery.js/dist/css/lg-transitions.css",
              "node_modules/aos/dist/aos.css",
              "src/assets/css/microtip.css"
            ],
            "scripts": [
              "src/assets/plugins/jquery-1.12.4.min.js",
              "node_modules/lightgallery.js/dist/js/lightgallery.js",
              "node_modules/lg-thumbnail.js/dist/lg-thumbnail.min.js",
              "node_modules/lg-share.js/dist/lg-share.min.js",
              "node_modules/lg-fullscreen.js/dist/lg-fullscreen.min.js",
              "node_modules/lg-zoom.js/dist/lg-zoom.min.js",
              "node_modules/lg-autoplay.js/dist/lg-autoplay.min.js",
              "src/assets/plugins/owl-carousel/owl.carousel.min.js",
              "src/assets/plugins/bootstrap4/js/bootstrap.min.js",
              "src/assets/plugins/imagesloaded.pkgd.js",
              "src/assets/plugins/masonry.pkgd.min.js",
              "src/assets/plugins/jquery.matchHeight-min.js",
              "src/assets/plugins/slick/slick/slick.min.js",
              "src/assets/plugins/jquery-bar-rating/dist/jquery.barrating.min.js",
              "src/assets/plugins/jquery-ui/jquery-ui.min.js",
              "src/assets/plugins/sticky-sidebar/dist/sticky-sidebar.min.js",
              "src/assets/plugins/select2/dist/js/select2.full.min.js",
              "src/assets/plugins/popper.min.js",
              "src/assets/js/main.js",
              "src/custom.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/select2/dist/js/select2.min.js",
              "node_modules/aos/dist/aos.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4mb",
                  "maximumError": "4mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hgp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hgp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hgp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json",
              "tsconfig.server.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "hgp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hgp:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/hgp/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "hgp:build",
            "serverTarget": "hgp:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "hgp:build:production",
              "serverTarget": "hgp:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "hgp:build:production",
            "serverTarget": "hgp:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    }
  },
  "defaultProject": "hgp",
  "cli": {
    "analytics": "eec0429f-5805-4406-97d0-85c0e5557abb"
  }
}
